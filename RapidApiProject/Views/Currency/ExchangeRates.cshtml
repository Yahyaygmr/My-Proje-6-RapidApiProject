@model List<RapidApiProject.Models.ExchangeRatesViewModel.Exchange_Rates>
@{
    ViewData["Title"] = "ExchangeRates";
}


<div class="container">
    <h1>Exchange Rates(@ViewBag.currencyCode)</h1><a href="/Currency/Index/">Go Back</a>
    <hr />
    <table class="table table-hover">
        <tr>
            <th>#</th>
            <th>Döviz</th>
            <th>Kur</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>#</td>
                <td>@item.currency</td>
                <td>@item.exchange_rate_buy</td>
            </tr>
        }
    </table>
</div>


